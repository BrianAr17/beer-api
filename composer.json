{
  "name": "frostybee/slim-template",
  "description": "A Slim framework skeleton for implementing REST-based Web services.",
  "type": "project",
  "homepage": "https://github.com/frostybee/slim-template",
  "minimum-stability": "stable",
  "license": "MIT",
  "keywords": [
    "php",
    "slim-micro-framework",
    "rest-api",
    "slim4",
    "slim",
    "rest",
    "api"
  ],
  "authors": [
    {
      "name": "FrostyBee",
      "homepage": "https://github.com/frostybee",
      "role": "Developer"
    }
  ],
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "require": {
    "php": "^8.2 || ^8.3 || ^8.4",
    "ext-json": "*",
    "ext-pdo": "*",
    "fig/http-message-util": "^1.1",
    "firebase/php-jwt": "^6.11",
    "frostybee/valicomb": "^1.0",
    "guzzlehttp/guzzle": "^7.10",
    "monolog/monolog": "^3.9",
    "nyholm/psr7": "^1.8",
    "nyholm/psr7-server": "^1.1",
    "php-di/php-di": "^7.1",
    "slim/slim": "^4.15"
  },
  "config": {
    "process-timeout": 0,
    "sort-packages": true
  },
  "scripts": {
    "start": "php -S localhost:8080 -t public public/index.php"
  }
}
